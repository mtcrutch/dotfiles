[user]
  name = Matt Crutchfield
  email = mtcrutch@gmail.com
[core]
  excludesfile = ~/.gitignore
  attributesfile = ~/.gitattributes
  precomposeUnicode = true
[commit]
  verbose = true
[pull]
  rebase = true
[push]
  default = current
  followTags = true
[init]
  defaultBranch = main
[rebase]
  autosquash = true